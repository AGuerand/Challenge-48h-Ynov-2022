<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="stylesheet" type="text/css" href="style/style.css"/>
		<link rel="stylesheet" type="text/css" href="style/card.css" />
		<link rel="stylesheet" type="text/css" href="style/navbar.css"/>
        <link rel="stylesheet" type="text/css" href="style/searchbar.css"/>
	</head>

	{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
	{% block stylesheets %}
		{{ encore_entry_link_tags('app') }}
	{% endblock %}

	{% block javascripts %}
		{{ encore_entry_script_tags('app') }}
	{% endblock %}
</html>
<body style="background-color:#e8e8e8">

<div class="">
    <div class="">
    <!-- HEADER -->
        <header>
            <div class="header" id="myHeader">
                <!-- LOGO -->
                <a href="/" class="logo">
                    <div class="logo-containerr">
                        {# <img src="../assets/images/logo.png" alt="Home" class="logo-img"></img> #}
                        <h1 class="logo-txt">Meet</h1>
                    </div>
                </a>
                <!-- END LOGO -->

                <!-- SEARCHBAR -->
                <div class="search-container">
                    <form method="GET" action="/">
                        <img class="search-icon" src="data:image/svg+xml;utf8;base64,">
                        <input placeholder="Search.." type="search" class="search" style="background: white;" list="listArtists" id="searchbarr" name="query">
                    </form>
                </div>
                <!-- END SEARCHBAR -->

                <!-- PANNEL -->
                
                <div class="right-container">
                    {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                        {{ app.user.getUsername }}
                        <a class="register" href="/form">Form</a>
                        <a class="inactive" href="/logout">Log out</a>
                    {% else %}
                        <a class="register" href="/register">Register</a>
                        <a class="active" href="/login">Log in</a> 
                    {% endif %}
                    {#  <a class="active" href="/login">Log in</a> 
                        <a class="inactive" href="/logout">Log out</a> #}
                </div>
                <!-- END PANNEL -->
            </div>
        </header>
    </div>
</div>
{# </ul></body></div></div></body></nav><div class="container"> {% block body %}{% endblock %}</div></body></html> #}
{% block body %}{% endblock %}
